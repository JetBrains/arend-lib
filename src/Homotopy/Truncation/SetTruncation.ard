\import Homotopy.Truncation

\data SetTrunc (A : \Type)
  | point A
  | trunc (x y : SetTrunc A) (p q : x = y) (i j : I) \elim i, j {
    | left, j => p @ j
    | right, j => q @ j
    | i, left => x
    | i, right => y
  }

\instance SetTruncTruncated {A : \Type} : Truncated_-1+ 1 (SetTrunc A)
  | isTruncated x y p q => path (\lam i => path (trunc x y p q i))

