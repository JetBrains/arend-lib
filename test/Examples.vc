 \data TrS' (A : \Type) 
      | inS' A 
      | truncS' (a a' : TrS' A) (p q : a = a') (i j : I) => \elim i, j {
        | left,  _ => a
        | right, _ => a'
        | i, left  => p @ i
        | i, right => q @ i
      }

\function
set_trunc_test (A : \Type) (a a' : TrS' A) (p q : a = a') : TrS' A => truncS' a a' p q left left


\function
set_trunc_test' (A : \Type) (a a' : TrS' A) (p q : a = a') : p = q => path (\lam i => path (\lam j => truncS' a a' p q j i))
