\open Paths

\data S1 
	| base
	| loop I 
    \with 
	| loop left => base
	| loop right => base

\function
loopP : (base = base) => path (\lam i => loop i)

\function
arc (i : I) : (base = loop i) => path (\lam j => loop (squeeze i j))

\function
S1-ind (B : S1 -> \Type0) (b : B base) (l : transport B loopP b = b) (x : S1) : (B x) <= \elim x
       | base => b
       | loop i => pathOver l @ i

       


 