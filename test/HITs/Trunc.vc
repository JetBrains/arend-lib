    \function rlemma {Y : \oo-Type} {x1 x2 : Y} (p q : x1 = x2) (C : p *> inv p = q *> inv p) : r'' p p q C = r' p p q C
        => Jl (\lam x2' p' => \Pi (q' : x1 = x2') (C : p' *> inv p' = q' *> inv p') -> r'' p' p' q' C = r' p' p' q' C) (\lam q' C => idp) p q C

    \function h-instance {X : Pointed} {n : Nat} (Xc : (1 + n) `connected-type` X.1) (q : north {X.1} = south) : h-data {X} {n} q =>
        \let | mx0 : north = south => path (merid X.2)
             | mx0i : south = north => inv mx0
             | PP : X.1 -> X.1 -> (2 + n + n) `Types => \lam x1 x2 => (
                 \let C0 => path (merid x2) *> mx0i = q *> inv (path (merid x1))
