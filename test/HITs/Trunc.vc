    \function r'-linv {Y : \oo-Type} {x1 x2 : Y} (p' p q : x1 = x2) (C : p' *> inv p = q *> inv p) : r'-inv p' p q (r' p' p q C) = C
        => Jl (\lam x2 p => \Pi (p' q : x1 = x2) (C : p' *> inv p = q *> inv p) -> r'-inv p' p q (r' p' p q C) = C) (\lam p' q (C : p' = q) => concat-idp _ *> concat-idp _) p p' q C

