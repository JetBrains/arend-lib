       | T : \Pi (q : north = north) -> codeN n q = codeS n (q *> path (merid x1)) => \lam q => pmap (codeN n) (pp q) *> T' (q *> (path (merid x1)))
       \in (coe (\lam i => \Pi (p : north = merid x1 i) -> codeN n (p *> (coe (\lam j => merid x1 j = north) idp i)) = codeS n (p *> (path (\lam j => merid x1 (squeeze-r i j))))) T i p) @ i

