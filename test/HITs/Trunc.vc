    \function codeN {X : Pointed} (n : Nat) (p : north {X.1} = north) : \oo-Type => Trunc (1 + n + n) (fib sigma p)

    \function codeS {X : Pointed} (n : Nat) (p : north {X.1} = south) : \oo-Type => Trunc (1 + n + n) (fibS p)

