    \function h-data2 {X : Pointed} {n : Nat} (x1 : X.1) (i : I) (q : north {X.1} = merid x1 i) =>
      \Sigma (h : codeN n (q *> inv (k x1 i)) -> codeS2 n x1 i q) (isequiv h)

    \function T'' {X : Pointed} {n : Nat} (Xc : (1 + n) `connected-type` X.1) (x1 : X.1) (i : I) (q : north = merid x1 i) : h-data2 {X} {n} x1 i q
       => \let | PP : X.1 -> X.1 -> (2 + n + n) `Types => \lam x1 x2 =>
              \let C0 => path (merid x2) *> inv (path (merid X.2)) = q *> inv (k x1 i)
              \in (C0 -> codeS2 n x1 i q, ofHlevel-codom C0 (\lam _ => codeS2 n x1 i q) (2 + n + n) (\lam _ => ofHlevel-Trunc (1 + n + n) (fibS2 x1 i q)))
          \in {?}

    \function r' {Y : \oo-Type} {x1 x2 : Y} (p' p q : x1 = x2) (C : p' *> inv p = q *> inv p) : p' = q =>
       pmap (\lam pp => p' *> pp) (inv (inv-concat p)) *> inv (concat-assoc p' (inv p) p) *> pmap (\lam pp => pp *> p) C *> concat-assoc q (inv p) p *> pmap (q *>) (inv-concat p)

